Algoritmo "Creuza01"

Var
   ano_atual, ano_nasc: inteiro
   idade: inteiro
Inicio
      Escreva ("Em que ano nós estamos? ")
      Leia (ano_atual)
      Escreva ("Em que ano eu nasci? ")
      Leia (ano_nasc)
      Idade <- (ano_atual - ano_nasc)
      Escreva ("Eu tenho", Idade, " anos.")
Fimalgoritmo