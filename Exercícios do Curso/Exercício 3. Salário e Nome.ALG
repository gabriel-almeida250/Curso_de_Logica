algoritmo "semnome"
// Disciplina : Lógica de Programação
// Exercício 3: Sálario e Nome
var
   nome: caractere
   salario: caractere
inicio
   nome <- "Maria do Carmo"
   salario <- "R$1850,45"
   escreval ("Nome do Funcionário: ", nome)
   escreval ("Salário: ", salario)
   escreva ("O funcincionário(a) ", nome, " teve um salário de ", salario, " no mês de junho.")
fimalgoritmo